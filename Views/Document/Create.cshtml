@model PROG6212_POE_CMCS.Models.Document
@{
    ViewData["Title"] = "Upload Document";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="FileName" class="control-label">Select File</label>
        <input asp-for="FileName" type="file" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Upload Document" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>
